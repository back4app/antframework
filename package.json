{
  "name": "@back4app/antframework",
  "version": "1.0.3",
  "engines": {
    "node": ">=10.8"
  },
  "preferGlobal": true,
  "description": "Ant Framework - Serverless microservices made easy",
  "keywords": [
    "microservices",
    "serverless",
    "api",
    "graphql"
  ],
  "files": [
    "bin",
    "lib",
    "index.js",
    "LICENSE",
    "package-lock.json",
    "package.json"
  ],
  "main": "index.js",
  "scripts": {
    "install": "cd ./lib/plugins/graphQL/templates/server/default && npm install && cd ../../../../serverless && npm install && cd ./templates/serverless/default && npm install && cd ../../../../../../",
    "lint": "eslint ./",
    "pretest": "npm run lint",
    "test": "jest",
    "posttest": "rm -r ./spec/support/out",
    "apidocs": "jsdoc -c ./jsdoc.json",
    "docs": "npm run apidocs"
  },
  "bin": {
    "ant": "./bin/ant.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/back4app/antframework.git"
  },
  "author": "Back4App Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/back4app/antframework/issues"
  },
  "homepage": "https://github.com/back4app/antframework#readme",
  "devDependencies": {
    "eslint": "^5.2.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "jest": "^23.4.2",
    "jszip": "^3.1.5",
    "mustache": "^2.3.0",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2",
    "yaml": "^1.0.0-rc.7",
    "yargs": "^12.0.1",
    "graphql": "^0.13.0"
  }
}
