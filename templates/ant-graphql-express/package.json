{
  "name": "@back4app/ant-graphql-express",
  "version": "0.0.15",
  "engines": {
    "node": ">=10.10"
  },
  "description": "Express.js server template to run a GraphQL API for Ant Framework - Serverless microservices made easy",
  "keywords": [
    "ant",
    "graphql",
    "express",
    "template",
    "graphiql",
    "server",
    "microservices",
    "api",
    "functions",
    "reactive",
    "serverless"
  ],
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "doc": "docs",
    "test": "spec"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "LICENSE",
    "package.json"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --config ./node_modules/@back4app/ant-util-tests/.eslintrc.js --ignore-path ./node_modules/@back4app/ant-util-tests/.eslintignore",
    "pretest": "npm run lint",
    "test": "jest --config ./node_modules/@back4app/ant-util-tests/jest.config.js --rootDir ./",
    "posttest": "rm -rf ./spec/support/out",
    "apidocs": "jsdoc -c ./node_modules/@back4app/ant-util-docs/jsdoc.json ./*",
    "docs": "npm run apidocs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/back4app/antframework.git"
  },
  "author": "Back4App Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/back4app/antframework/issues"
  },
  "homepage": "https://github.com/back4app/antframework/tree/master/templates/ant-graphql-express#readme",
  "dependencies": {
    "@back4app/ant": "^0.0.15",
    "@back4app/ant-graphql": "^0.0.15",
    "@back4app/ant-util": "^0.0.15",
    "@back4app/ant-util-rxjs": "^0.0.15",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.0",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "devDependencies": {
    "@back4app/ant-util-docs": "^0.0.15",
    "@back4app/ant-util-tests": "^0.0.15",
    "eslint": "^5.6.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  },
  "gitHead": "cc52e334f800e4906bef481e692c49d5246ebe86"
}
